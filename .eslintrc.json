{
        "parser": "babel-eslint",
        "env": {
                "browser": true,
                "es6": true,
                "node": true,
                "jquery": true
        } ,
        "extends": "eslint:recommended",
        "parserOptions": {
                "sourceType": "module"
        },
        "rules": {
                "indent": [ 2, 4 , {"SwitchCase": 1}],
                "linebreak-style": [2,"unix"],
                "quotes": [2,"double"],
                "semi": [ 2,"always"],
                "no-console": [0]
        },
        "globals": {
            "Homey":   true
        }
}
